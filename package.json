{
   "name": "wasaby-cli",
   "version": "23.3000.0",
   "description": "Build and run unit tests for wasaby",
   "scripts": {
      "test": "jest --roots=test",
      "coverage": "jest --coverage"
   },
   "repository": {
      "type": "git",
      "url": "git@git.sbis.ru:saby/wasaby-cli.git"
   },
   "bin": {
      "wasaby-cli": "./cli.js"
   },
   "jest": {
      "roots": [
         "src",
         "test"
      ],
      "collectCoverageFrom": [
         "**/*.js"
      ],
      "coverageDirectory": "./coverage",
      "coverageReporters": [
         [
            "html",
            {
               "subdir": "coverage"
            }
         ]
      ]
   },
   "dependencies": {
      "@testing-library/react": "12.1.5",
      "@testing-library/user-event": "14.0.1",
      "@types/jest": "27.5.1",
      "webdriverio": "7.20.9",
      "@wdio/cli": "7.20.9",
      "@wdio/local-runner": "7.20.9",
      "@wdio/reporter": "7.20.7",
      "@wdio/junit-reporter": "7.20.7",
      "@wdio/mocha-framework": "7.20.7",
      "@wdio/devtools-service": "7.20.9",
      "body-parser": "1.19.2",
      "cookie-parser": "1.4.6",
      "es5-ext": "0.10.53",
      "express": "4.17.3",
      "fs-extra": "10.0.1",
      "glob": "7.2.0",
      "open": "8.4.0",
      "openssl-nodejs": "1.0.5",
      "p-map": "4.0.0",
      "serve-static": "1.14.2",
      "spdy": "4.0.2",
      "xml2js": "0.4.23",
      "connect": "3.7.0",
      "jest": "27.5.1",
      "jest-junit": "13.0.0",
      "jest-environment-jsdom": "27.5.1",
      "jest-environment-node": "27.5.1",
      "saby-typescript": "git+https://git.sbis.ru/saby/typescript.git#rc-23.3000",
      "saby-units": "git+https://git.sbis.ru/saby/units.git#rc-23.3000",
      "sbis3-builder": "git+https://git.sbis.ru/saby/builder.git#rc-23.3000"
   },
   "resolutions": {
      "es5-ext": "0.10.53"
   },
   "overrides": {
      "es5-ext": "0.10.53"
   }
}
